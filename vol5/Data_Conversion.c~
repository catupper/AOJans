#include<stdio.h>
int main(){
  int n, m, i, j
  char a[1000], b[1000];
  while(1){
    scanf("%d",&n);
    if(n == 0)return 0;
    for(i = 0;i < n;i++){
      scanf(" %c",a + i);
    }
    scanf("%d", &m);
    for(i = 0;i < m;i++){
      char c;
      scanf(" %c",&c);
      for(j = 0;j < n;j++){
	if(c == a[i]){
	  c = b[i];
	  break;
	}
      }
      printf("%c",c);
    }
    printf("\n");
  }
}
